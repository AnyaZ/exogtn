<?xml version="1.0"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">

    <localRepository>${env.HOME}/.m2_EPP</localRepository>

    <mirrors>
        <!--<mirror>-->
            <!--<id>internal-repository</id>-->
            <!--<name>Maven Repository Manager running on repo.mycompany.com</name>-->
            <!--<url>http://download.devel.redhat.com/brewroot/repos/jboss-epp-5-build/latest/maven</url>-->
            <!--<mirrorOf>*</mirrorOf>-->
        <!--</mirror>-->
        <mirror>
            <id>internal-repository1</id>
            <name>Maven Repository Manager running on jboss.org</name>
            <url>http://repository.jboss.org/nexus/content/groups/public-jboss</url>
            <mirrorOf>*</mirrorOf>
        </mirror>
    </mirrors>
    <profiles>
        <profile>

            <id>jbrep</id>

            <repositories>
                <repository>
                    <id>jboss-int</id>
                    <url>http://repository.jboss.org/nexus/content/groups/public-jboss</url>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                </repository>
            </repositories>
        </profile>
    </profiles>
</settings>
